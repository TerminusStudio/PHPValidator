{
    "name": "terminusstudio/phpvalidator",
    "description": "A php validator using respect validator. Built for Slim",
    "type": "library",
    "require": {
        "php": ">=7.4",
        "psr/http-message": "^1.0",
        "psr/http-server-handler": "^1.0",
        "respect/validation": "2.0.17"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.5",
        "phpunit/phpunit": "^9.0",
        "twig/twig": "^3.1"
    },
    "autoload": {
        "psr-4": {
            "TS\\PHPValidator\\": "src"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-dev": "0.0.x-dev"
        }
    }
}

{
    "name": "terminusstudio/phpvalidator",
    "description": "A php validator using respect validator. Built for Slim",
    "type": "library",
    "require": {
        "php": ">=7.4",
        "psr/http-message": "^1.0"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.5",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "TS\\PHPValidator\\": "src"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-dev": "0.0.x-dev"
        }
    }
}
